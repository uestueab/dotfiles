    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Enter>"
            ],
            "after": [
                "i",
                "<Enter>"
            ]
        },
        {
            "before": [
                "<Backspace>"
            ],
            "after": [
                "i",
                "<Backspace>"
            ]
        },
        {
            "before": [
                "<Space>"
            ],
            "after": [
                "i",
                "<Space>"
            ]
        }

    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<c-z>"
            ],
            "after": [
                "<nop>"
            ]
        },
        {
            "before": [
                "<C-c>"
            ],
            "after": [
                "\"",
                "+",
                "y"
            ]
        }
