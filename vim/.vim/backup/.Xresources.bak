! ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
! Rofi / Color Scheme / Star Wars
! ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

#rofi.color-enabled: true
#rofi.modi: run,drun
#rofi.fullscreen: true


! Property                Background            Foreground           Background Alt          Highlight BG         Highlight FG
#rofi.color-window:        #020005,              #e7dceb,             #020005,                #3aa274,             #020005,
#rofi.color-normal:        #020005,              #e7dceb,             #020005,                #3aa274,             #020005,
#rofi.color-active:        #020005,              #e7dceb,             #020005,                #3aa274,             #020005,
#rofi.color-urgent:        #020005,              #68dccc,             #020005,                #3aa274,             #020005,




#rofi.modi: run,drun
#rofi.color-enabled:     true
!                       'background', 'border'
#rofi.color-window:      argb:dc111111, argb:dc111111
! State:                'bg',          'fg',   'bgalt',       'hlbg',        'hlfg'
#rofi.color-normal:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ffefa211
#rofi.color-urgent:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ffefa211
#rofi.color-active:      argb:00333333, #d2deab, argb:00333333, argb:00333333, argb:ffefa211
#rofi.separator-style:   dash
#rofi.fullscreen:        true
#rofi.padding:           300
#rofi.hide-scrollbar:    true
#rofi.line-margin:       5
#rofi.lines:             7
#rofi.fake-transparency: false
#rofi.yoffset:           0
#rofi.xoffset:           0
#rofi.width:             100
#rofi.opacity:           80
#rofi.font:              xos4 Terminus 15


! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #acafa9 , argb:00000000,  #673AB7 ,  #1B2B34
! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #acafa9 , argb:00000000,  #673AB7 ,  #1B2B34
! Color scheme for active row
rofi.color-active:                   argb:00000000,  #6699CC , argb:00000000,  #673AB7 ,  #1B2B34
! Color scheme window
rofi.color-window:                   argb:dc111111,  #1e1e1e ,  #1e1e1e
! Separator style (none, dash, solid)
rofi.separator-style:               solid
rofi.fullscreen:                    true
rofi.hide-scrollbar:                true
rofi.width:                         100
rofi.padding:                       300
rofi.font:              xos4 Terminus 15
rofi.modi: run,drun
